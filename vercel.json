{
  "redirects": [
    {
      "source": "/mvp/index.html",
      "destination": "/mvp",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/coming-soon.html"
    },
    {
      "source": "/mvp",
      "destination": "/index.html"
    },
    {
      "source": "/mvp/:path*",
      "destination": "/:path*"
    }
  ],
  "headers": [
    {
      "source": "/mvp",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow, noarchive"
        }
      ]
    },
    {
      "source": "/mvp/:path*",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow, noarchive"
        }
      ]
    }
  ]
}
